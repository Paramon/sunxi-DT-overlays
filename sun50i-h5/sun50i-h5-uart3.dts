/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun50i-h5";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			serial3 = "/soc/serial@01c28c00";
		};
	};

	fragment@1 {
		target = <&uart3>;
		 __overlay__ {
			pins = "PA13", "PA14";
			linux,phandle = <0x19>;
			phandle = <0x19>;
			
			status = "okay";
		};
	};
};
